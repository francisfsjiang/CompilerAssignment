//bigger circle
ORIGIN IS (800,250);
FOR T FROM 0 TO 2*PI STEP 0.1 DRAW (50*cos(T),50*sin(T)) COLOR (127*sin(T)+128,0,0) RADIUS T;

//y=x**3
